# Local-only environment variables (do not commit your real secrets).
# Copy to .env (already gitignored) and set values for local runs.

# Personal Access Token or pipeline token
ADO_TOKEN=your-token-here

# Organization URL, e.g. https://dev.azure.com/your-org/
ADO_ORG_URL=https://dev.azure.com/your-org

# Project name
ADO_PROJECT=YourProject

# Optional build metadata for local runs
ADO_BUILD_NUMBER=LocalRun
